@inbook{Laser_cooling_techniques,
	author = { C.   Champenois },
	title = {Trapped Charged Particles},
	booktitle = {Trapped Charged Particles},
	chapter = {6},
	pages = {117-145},
	doi = {10.1142/9781786340139_0006},
	URL = {https://www.worldscientific.com/doi/abs/10.1142/9781786340139_0006},
	eprint = {https://www.worldscientific.com/doi/pdf/10.1142/9781786340139_0006},
	abstract = { This chapter gathers two lectures which were dedicated to processes involved in general laser cooling and to laser cooling technics specific to trapped particles. In this last lecture, assumptions about the atomic motion were made, which were directly inspired by the situation of ions trapped in an Radiofrequency (RF) quadrupole trap. Nevertheless, the motion of ions in Penning traps has a lot to share with the motion in RF quadrupole traps, mostly its harmonic nature. As a consequence, methods explained in this last context can be adapted to ions in Penning traps. The spirit of this chapter is to start from the fundamental basic equations to allow the reader to derive the formulae in his own specific context. One of the goals is to identify the parameters that control the relevant orders of magnitude that need to be estimated before building a laser cooling set-up. }
}

